{
	"name": "dxcherry",
	"displayName": "dxCherry",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:exampleView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "test",
					"title": "test",
					"icon": "resources/dark/checked.svg"
				}
			]
		},
		"views": {
			"test": [
				{
					"id": "exampleView",
					"name": "exampleView"
				}
			]
		},
		"commands": [
			{
				"command": "start",
				"title": "Create pull requests",
				"icon": {
					"light": "resources/light/start.svg",
					"dark": "resources/dark/start.svg"
				}
			},
			{
				"command": "refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "start",
					"group": "navigation",
					"when": "view == exampleView"
				},
				{
					"command": "refresh",
					"group": "navigation",
					"when": "view == exampleView"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@octokit/rest": "^18.0.9",
		"@octokit/types": "^5.5.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"dotenv": "^8.2.0",
		"eslint": "^7.14.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"git-branch": "^2.0.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.21.0"
	}
}
